\newcommand{\assemble}{\mathop{\calc@currsize{A}}}
\def\calc@currsize#1{%
   \ifx\@currsize\tiny%
      \lower@factor{0.88}{0.65}{0.7}{0.7}%
      \adjust@size{cmss}{12}{8}{7}{7}{#1}%
   \else%
   \ifx\@currsize\scriptsize%
      \lower@factor{0.85}{0.63}{0.65}{0.7}%
      \adjust@size{cmss}{14.4}{10.95}{8}{7}{#1}%
   \else%
   \ifx\@currsize\footnotesize%
      \lower@factor{0.82}{0.7}{0.6}{0.7}%
      \adjust@size{cmss}{17.28}{14.4}{9}{7}{#1}%
   \else%
   \ifx\@currsize\small%
      \lower@factor{0.9}{0.75}{0.72}{0.72}%
      \adjust@size{lcmss}{16.59}{13.82}{10}{8}{#1}%
   \else%
   \ifx\@currsize\normalsize%
      \lower@factor{0.95}{0.81}{0.75}{0.75}%
      \adjust@size{lcmss}{19.907}{16.59}{10}{8}{#1}%
   \else%
   \ifx\@currsize\large%
      \lower@factor{0.95}{0.8}{0.81}{0.8}%
      \adjust@size{lcmss}{23.89}{19.907}{13.82}{10}{#1}%
   \else%
   \ifx\@currsize\Large%
      \lower@factor{0.95}{0.79}{0.8}{0.8}%
      \adjust@size{lcmss}{28.66}{23.89}{16.59}{13.82}{#1}%
   \else%
   \ifx\@currsize\LARGE%
      \lower@factor{0.95}{0.82}{0.82}{0.8}%
      \adjust@size{lcmss}{34.4}{28.66}{19.907}{16.59}{#1}%
   \else%
   \ifx\@currsize\huge%
      \lower@factor{0.95}{0.8}{0.8}{0.81}%
      \adjust@size{lcmss}{41.28}{34.4}{28.66}{23.89}{#1}%
   \else%
   \ifx\@currsize\Huge%
      \lower@factor{0.95}{0.82}{0.8}{0.79}%
      \adjust@size{lcmss}{41.28}{41.28}{34.4}{28.66}{#1}%
   \fi\fi\fi\fi\fi\fi\fi\fi\fi\fi%
}
\def\lower@factor#1#2#3#4{%
   \def\f@lower{#1}%
   \def\tf@lower{#2}%
   \def\sf@lower{#3}%
   \def\ssf@lower{#4}%
}
\def\adjust@size#1#2#3#4#5#6{%
   \mathchoice%
  {\hbox{\contents@hbox{#1}{#2}{\f@lower}{#6}}}%
  {\hbox{\contents@hbox{#1}{#3}{\tf@lower}{#6}}}%
  {\hbox{\contents@hbox{#1}{#4}{\sf@lower}{#6}}}%
  {\hbox{\contents@hbox{#1}{#5}{\ssf@lower}{#6}}}%
   \check@mathfonts%
}
\def\contents@hbox#1#2#3#4{%
   \edef\sfdefault{#1}\edef\f@size{#2}\sffamily%
   \selectfont{\@unit\lower #3\dp\unit@box\hbox{#4}}%
}
\newbox\unit@box
\def\@unit{\setbox\unit@box\hbox{(}}


